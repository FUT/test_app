<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Age:</strong>
  <%= @user.age %>
</p>

<p>
  <strong>Name2:</strong>
  <%= @user.name2 %>
</p>

<p>
  <strong>Photo:</strong>
  <%= image_tag @user.photo %>
</p>

<p>
  <% @user.posts.each do |post| %>
    <div class='post'>
      <strong><%= post.title %></strong>
      <%= post.body %>
    </div>
  <% end%>
</p>
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
